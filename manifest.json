{
  "name": "Github Easy PR",
  "description": "Adds Approve button in Pull request comments",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*.github.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}
